datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model VisitCount {
  id    Int    @id @default(autoincrement())
  page  String @unique
  count Int    @default(0)
}
